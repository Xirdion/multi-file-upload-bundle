{
  "name": "xirdion/multi-file-upload-bundle",
  "type": "contao-bundle",
  "description": "Contao Bundle to upload multiple files with dropzonejs",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Thomas Voggenreiter",
      "email": "t.voggenreiter01@gmail.com",
      "homepage": "https://github.com/Xirdion"
    }
  ],
  "require": {
    "php": "^7.3",
    "contao/core-bundle": "^4.8"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1",
    "contao/test-case": "^3.0",
    "friendsofphp/php-cs-fixer": "^2.6",
    "php-http/guzzle6-adapter": "^1.1",
    "phpunit/phpunit": "^7.5",
    "symfony/phpunit-bridge": "^4.3"
  },
  "extra": {
    "contao-manager-plugin": "Xirdion\\MultiFileUploadBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Xirdion\\MultiFileUploadBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Xirdion\\MultiFileUploadBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/Xirdion/multi-file-upload-bundle/issues",
    "source": "https://github.com/Xirdion/multi-file-upload-bundle"
  }
}